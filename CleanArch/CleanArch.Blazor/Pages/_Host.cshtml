@page "/"
@inject Microsoft.AspNetCore.Antiforgery.IAntiforgery Access
@namespace CleanArch.Blazor.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
    var tokens = new TokenProvider
    {
        AcessToken = Access.GetAndStoreTokens(HttpContext).RequestToken
    };
}

<component type="typeof(App)" param-InitialState="tokens" render-mode="ServerPrerendered" />
